FUNC main() INT
    print("Please enter your age:")
    INT age = input()
    print("Your are %d years old?\n",age)
    IF age < 0 THEN
        print("Dont be silly...\n")
        RETURN 1
    END
    IF age < 1 THEN
        print("Just born and already using a computer. Sure thing...\n")
        RETURN 1
    END
    IF age > 23 THEN
        print("Fuck! You're old as hell!\n")
    ELSE
        print("Astonishingly young!!!\n")
    END
    RETURN 0
END