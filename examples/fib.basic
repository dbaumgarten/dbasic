FUNC main() INT
    print("How many?:")
    INT dest = input()
    INT x = 0
    WHILE x < dest DO
        print("%d,",fib(x))
        x = x+1
    END
    print("\n")
    RETURN 0
END

FUNC fib(INT n) INT
    IF n < 2 THEN
        RETURN 1
    ELSE
        RETURN fib(n-1)+fib(n-2)
    END
    RETURN 0
END